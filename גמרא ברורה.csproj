  <ItemGroup>
    <Resource Include="Resources\contradiction - transparent.png" />
    <Resource Include="Resources\contradiction.png" />
    <Resource Include="Resources\other-tranparent.png" />
    <Resource Include="Resources\other.png" />
    <Resource Include="Resources\proof - transparent.png" />
    <Resource Include="Resources\proof.png" />
    <Resource Include="Resources\question-transparent.png" />
    <Resource Include="Resources\question.png" />
