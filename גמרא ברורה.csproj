    <None Remove="Resources\other-tranparent.png" />
    <None Remove="Resources\other.png" />
    <None Remove="Resources\proof - transparent.png" />
    <None Remove="Resources\proof.png" />
    <None Remove="Resources\question-transparent.png" />
    <None Remove="Resources\question.png" />
    <None Remove="Resources\statement - transparent.png" />
    <None Remove="Resources\statement.png" />
